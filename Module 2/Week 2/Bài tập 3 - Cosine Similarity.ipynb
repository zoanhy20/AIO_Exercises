   "source": [
    "import numpy as np\n",
    "\n",
    "def compute_cosine(v1, v2):\n",
    "    cos_sim = np.dot(v1, v2) / (np.linalg.norm(v1) * np.linalg.norm(v2))\n",
    "    \n",
    "    return cos_sim"
   ],
